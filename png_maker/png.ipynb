   "source": [
    "def solution(A):\n",
    "  S = sum(A)\n",
    "  dp = {0}\n",
    "  \n",
    "  for a in A: \n",
    "    new_sums = set()\n",
    "    \n",
    "    for s in dp : \n",
    "      if s+a <= S//2: \n",
    "        new_sums.add(s+a)\n",
    "        \n",
    "    dp = dp.union(new_sums)\n",
    "    \n",
    "  return S - 2* max(dp)\n",
    "\n",
    "solution([1,2,3,4,5])\n"
   ]
  }
 ],
